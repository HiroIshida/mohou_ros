<launch>
  <include file="$(find pr2_gazebo)/launch/pr2_empty_world.launch">
    <arg name="gui" value="false" />
    <arg name="headless" value="true" />
  </include>

  <node name="image_republish" pkg="image_transport" type="republish" args="compressed raw">
   <remap from="in" to="/l_forearm_cam/image_color" />
   <remap from="out" to="/l_forearm_cam/image_color/compressed" />
  </node>

  <test pkg="mohou_ros" type="run_executor.py" test-name="excutor" time-limit="600"/>
</launch>
